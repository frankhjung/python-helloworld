{
  "version": 1,
  "formatters": {
    "standard": {
      "format": "%(asctime)s %(levelname)s %(message)s",
      "datefmt": "%Y-%m-%dT%H:%M:%SZ"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "standard"
    }
  },
  "loggers": {
    "": {
      "handlers": [
        "console"
      ],
      "propagate": false
    }
  }
}